    <file source-language="de" datatype="plaintext" original="messages" date="2014-06-02T11:47:10Z" product-name="w4Cookiebar">
        <header/>
        <body>
            <trans-unit id="w4_cookiebar.title" xml:space="preserve">
                <source>We use cookies</source>
                <target>Wir nutzen Cookies</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.note" xml:space="preserve">
                <source>We are using cookies to create an optimal user experience for our visitors.</source>
                <target>Wir verwenden Cookies, um unseren Besuchern ein optimales Benutzererlebnis zu ermöglichen.</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.link_text" xml:space="preserve">
                <source>Privacy policy</source>
                <target>Datenschutzerklärung</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.analytics_label" xml:space="preserve">
                <source>Analysis &amp; Marketing</source>
                <target>Analyse &amp; Marketing</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.analytics_text" xml:space="preserve">
                <source>To better understand what how visitors use our webpage.</source>
                <target>So verstehen wir besser, wie Besucher unsere Webseite nutzen.</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.maps_label" xml:space="preserve">
                <source>Location maps</source>
                <target>Maps (Standortkarten)</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.maps_text" xml:space="preserve">
                <source>This allows to show you maps from Google or Baidu.</source>
                <target>Damit wir online Karten von Google oder Baidu anzeigen können.</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.base_label" xml:space="preserve">
                <source>Essential cookies</source>
                <target>Grundlegende Cookies</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.base_text" xml:space="preserve">
                <source>These are required for the pages to function properly.</source>
                <target>Wir benötigen diese, damit diese Seite einwandfrei funktioniert.</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.allow_selected" xml:space="preserve">
                <source>Only selected and required</source>
                <target>Nur ausgewählte oder benötigte</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.allow_all" xml:space="preserve">
                <source>Allow all</source>
                <target>Alle erlauben</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.edit_button_title" xml:space="preserve">
                <source>Edit cookie settings</source>
                <target>Cookieeinstellungen bearbeiten</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.conditional_google_maps.popup_text" xml:space="preserve">
                <source>Please click the button so that Google Maps can be displayed.</source>
                <target>Bitte klicken Sie auf den Button, damit Google Maps angezeigt werden kann.</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.conditional_google_maps.popup_button" xml:space="preserve">
                <source>Allow Google Maps</source>
                <target>Erlaube Google Maps</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.conditional_marketing.popup_text" xml:space="preserve">
                <source>Please click the button so that Marketing can be displayed.</source>
                <target>Bitte klicken Sie auf den Button, damit Marketing inhalt angezeigt werden kann.</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.conditional_marketing.popup_button" xml:space="preserve">
                <source>Allow Marketing / Analysis content</source>
                <target>Erlauben Sie Marketing-/Analyse-Inhalte</target>
            </trans-unit>
            <trans-unit id="w4_cookiebar.edit" xml:space="preserve">
                <source>Show Edit cookie settings button</source>
                <target>Schaltfläche Cookie-Einstellungen bearbeiten anzeigen</target>
            </trans-unit>
        </body>
    </file>
